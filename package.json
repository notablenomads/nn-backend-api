{
  "name": "nn-backend-api",
  "version": "0.1.9",
  "description": "The backend API for the Notable Nomads backend",
  "author": "Mahdi Rashidi <m8rashidi@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Notable-Nomads/nn-backend-api.git"
  },
  "homepage": "https://github.com/Notable-Nomads/nn-backend-api",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create -d typeorm.config.ts",
    "migration:generate": "yarn typeorm migration:generate -d typeorm.config.ts",
    "migration:run": "yarn typeorm migration:run -d typeorm.config.ts",
    "migration:revert": "yarn typeorm migration:revert -d typeorm.config.ts",
    "migration:show": "yarn typeorm migration:show -d typeorm.config.ts",
    "release": "npx standard-version"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.738.0",
    "@google/generative-ai": "^0.21.0",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/terminus": "^10.3.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^10.4.15",
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.20",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@stylistic/eslint-plugin-js": "^2.13.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.12",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.0",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "lint-staged": "^15.4.3",
    "pinst": "^3.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix"
    ],
    "*.js": "eslint --cache --fix",
    "*.ts": "prettier --write"
  },
  "jest": {
    "moduleNameMapper": {
      "@root/(.*)": "<rootDir>/$1",
      "@test/(.*)": "<rootDir>/../test/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "lcov",
      "cobertura",
      "text"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "nestjs",
    "microservices",
    "framework"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "packageManager": "yarn@4.6.0",
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "section": "Code Refactoring"
      },
      {
        "type": "perf",
        "section": "Performance Improvements"
      },
      {
        "type": "test",
        "hidden": true
      }
    ],
    "commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commit/{{hash}}",
    "compareUrlFormat": "{{host}}/{{owner}}/{{repository}}/compare/{{previousTag}}...{{currentTag}}",
    "issueUrlFormat": "{{host}}/{{owner}}/{{repository}}/issues/{{id}}",
    "releaseCommitMessageFormat": "chore(release): {{currentTag}} [skip ci]",
    "skip": {
      "tag": false
    },
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ],
    "infile": "CHANGELOG.md",
    "header": "# Changelog\n\nAll notable changes to this project will be documented in this file.\n",
    "preset": "angular"
  }
}
